{
  "name": "h2w-cv-builder",
  "version": "1.0.1-SNAPSHOT",
  "description": "A template service for hack2work",
  "main": "src/app.js",
  "files": [
    "certs",
    "config",
    "public",
    "src",
    "gulpfile.js",
    "package.json",
    "package-lock.json"
  ],
  "scripts": {
    "clean": "gulp clean",
    "cover": "npm run cover:js && npm run cover:ts && npm run cover:browser",
    "cover:js": "nyc --nycrc-path nyc.js.config.js check-coverage",
    "cover:ts": "nyc --nycrc-path nyc.ts.config.js check-coverage",
    "cover:browser": "nyc --nycrc-path nyc.browser.config.js check-coverage",
    "build": "npm run clean && npm run build:gulp",
    "build:gulp": "gulp build",
    "mutation-tests": "npm run mutation-tests:js && npm run mutation-tests:ts && npm run mutation-tests:browser",
    "mutation-tests:browser": "stryker run stryker.browser.conf.js || true",
    "mutation-tests:js": "stryker run stryker.conf.js || true",
    "mutation-tests:ts": "TS_NODE_PROJECT=src/tsconfig.json stryker run stryker.ts.conf.js || true",
    "project:name": "echo $npm_package_name",
    "project:version": "echo $npm_package_version",
    "project:slug": "echo $npm_package_name-$npm_package_version",
    "quality:eslint": "eslint . --ext .js,.ts",
    "quality:eslint:fix": "eslint . --fix",
    "quality:sonar-scanner": "sonar-scanner",
    "regenerate-certs": "openssl req -x509 -newkey rsa:2048 -nodes -days 365 -sha256 -subj '/CN=localhost' -keyout certs/localhost-privkey.pem -out certs/localhost-cert.pem",
    "security:vulnerable-packages": "npm audit --registry=https://registry.npmjs.org",
    "start": "node src/app.js",
    "test": "npm run test:js && npm run test:ts && npm run test:browser",
    "test:js": "nyc --nycrc-path nyc.js.config.js mocha --config test/.mocharc.js --check-leaks",
    "test:browser": "nyc --nycrc-path nyc.browser.config.js mocha --config test/.browser.mocharc.js",
    "test:ts": "TS_NODE_PROJECT=src/tsconfig.json nyc --nycrc-path nyc.ts.config.js --include src/**/*.ts mocha --config test/.ts.mocharc.js --check-leaks"
  },
  "repository": {
    "type": "git",
    "url": "git://git@github.com/j4numbers/hack2work-cv-builder.git"
  },
  "bugs": {
    "url": "https://github.com/j4numbers/hack2work-cv-builder"
  },
  "keywords": [
    "hack2work",
    "cv-builder",
    "cv",
    "resume",
    "universal credit"
  ],
  "author": "Jayne Doe <jayne.doe@engineering.digital.dwp.gov.uk>",
  "license": "MIT",
  "dependencies": {
    "bunyan": "^1.8.14",
    "config": "^3.3.2",
    "govuk-frontend": "^3.10.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.13.2",
    "nunjucks": "^3.2.2",
    "restify": "^8.5.1",
    "restify-errors": "^8.0.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@dwp/eslint-config-base": "^5.0.1",
    "@dwp/eslint-config-mocha": "^1.2.1",
    "@dwp/nyc-config-base": "^1.0.0",
    "@stryker-mutator/core": "^5.2.1",
    "@stryker-mutator/mocha-runner": "^5.2.1",
    "@stryker-mutator/typescript-checker": "^5.2.1",
    "@types/bunyan": "^1.8.6",
    "@types/chai-as-promised": "^7.1.4",
    "@types/config": "0.0.38",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/mocha": "^8.0.4",
    "@types/mock-require": "^2.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/nunjucks": "^3.1.3",
    "@types/sinon": "^10.0.2",
    "@types/sinon-chai": "^3.2.5",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "clear-module": "^4.1.1",
    "eslint": "^7.30.0",
    "eslint-plugin-jsdoc": "^35.4.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-sass": "^5.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "import-fresh": "^3.2.2",
    "mocha": "^9.0.2",
    "mock-require": "^3.0.3",
    "nock": "^13.1.1",
    "nyc": "^15.1.0",
    "sass": "^1.35.1",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0",
    "ts-mock-imports": "^1.3.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "zombie": "^6.1.4"
  }
}
